<meta charset="UTF-8">
<meta name="description" content="{{ parameters.app_description }}">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<meta property="og:title" content="{{ parameters.app_title }}">
<meta property="og:description" content="{{ parameters.app_description }}">
<meta property="og:url" content="{{ parameters.app_url }}">
{#<meta property="og:image" content="{{ parameters.app_url }}{{ assetAliasPath('og_cover', true) }}">#}
<meta property="og:type" content="website">
<meta property="og:site_name" content="{{ parameters.app_name }}">
<meta property="al:android:app_name" content="{{ parameters.app_name }}">
<meta property="al:android:package" content="{{ parameters.app_package }}">
<meta property="al:android:url" content="{{ parameters.app_name | lower }}://">
<meta property="al:ios:app_name" content="{{ parameters.app_name }}">
{#<meta property="al:ios:app_store_id" content="">#}
<meta property="al:ios:url" content="{{ parameters.app_name | lower }}://">
<meta property="twitter:site" content="@{{ parameters.app_name | lower }}">
<meta property="twitter:title" content="{{ parameters.app_title }}">
<meta property="twitter:description" content="{{ parameters.app_description }}">
<meta property="twitter:card" content="summary">
{#<meta property="twitter:image" content="{{ parameters.app_url }}{{ assetAliasPath('og_cover', true) }}">#}