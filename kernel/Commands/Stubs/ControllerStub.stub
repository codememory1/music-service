<?php

namespace {namespace};

use Kernel\Controller\AbstractController;

/**
 * Class {className}
 * @package {namespace}
 */
class {className} extends AbstractController
{

    /**
     * @return bool
     */
    public function web(): bool
    {

        $this->get('response')
        	->setContent('Hello, {className}')->sendContent();

        return true;

    }

}